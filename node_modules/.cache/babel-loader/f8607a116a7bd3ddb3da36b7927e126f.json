AAA,iBAAA,GAAA,YAAA;AACA,KAdA;AAeA,IAAA,KAfA,mBAeA;AACA,UAAA,SAAA,GAAA,KAAA,SAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,WAAA,CAAA,OAAA,CAAA,IAAA,EAAA,GAAA,CAAA,IAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,WAAA,CAAA,OAAA,CAAA,IAAA,EAAA,GAAA,CAAA,CAAA;AACA,OAFA,CAAA;AAGA,WAAA,KAAA,CAAA,iBAAA,GAAA,SAAA;AACA,KApBA;AAqBA,IAAA,OArBA,qBAqBA;AACA,UAAA,WAAA,GAAA,KAAA,SAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,WAAA,GAAA,IAAA,CAAA,KAAA,GAAA,CAAA;AACA,OAFA,CAAA;AAGA,WAAA,KAAA,CAAA,iBAAA,GAAA,WAAA;AACA,KA1BA;AA2BA,IAAA,SA3BA,qBA2BA,GA3BA,EA2BA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,SAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA,KAhCA;AAiCA,IAAA,WAjCA,uBAiCA,GAjCA,EAiCA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,SAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA;AAtCA;AApBA,CAAA","sourcesContent":["<template>\n    <div>\n        <div class=\"row shop-navSelects bg-danger text-white flex-nowrap\">\n            <nav class=\"col-md-8 navbar navbar-expand-lg navbar-light\">\n                <span class=\"navbar-brand text-white\">篩選 :</span>\n                <button class=\"navbar-toggler\" type=\"button\" data-toggle=\"collapse\" data-target=\"#navbarNav\" aria-controls=\"navbarNav\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">\n                  <span class=\"navbar-toggler-icon\"></span>\n                </button>\n                <div class=\"collapse navbar-collapse\" id=\"navbarNav\">\n                  <ul class=\"navbar-nav\">\n                    <li class=\"nav-item\" @click.prevent=\"isRentMethod\">\n                      <a class=\"nav-link text-white\">所有可租借商品</a>\n                    </li>\n                    <li class=\"nav-item\" @click.prevent=\"isNew\">\n                      <a class=\"nav-link text-white\">最新推出</a>\n                    </li>\n                    <li class=\"nav-item\" @click.prevent=\"isCheap\">\n                      <a class=\"nav-link text-white\">優惠活動</a>\n                    </li>\n                  </ul>\n                </div>\n            </nav>\n            <div class=\"col-md-4 d-flex justify-content-end align-items-center\">\n                <div class=\"sortNav\">排序 :</div>\n                <div class=\"btn-group\">\n                    <button class=\"btn btn-secondary btn-sm dropdown-toggle bg-light text-dark ml-2 pr-4 pl-4\" type=\"button\" data-toggle=\"dropdown\" aria-haspopup=\"true\" aria-expanded=\"false\">\n                     {{ seletTxt }}\n                    </button>\n                    <div class=\"dropdown-menu dropdown-menu-right\">\n                        <button class=\"dropdown-item\" type=\"button\" @click=\"isAllItem('價格(低到高)')\">價格(低到高)</button>\n                        <button class=\"dropdown-item\" type=\"button\" @click=\"turnAllItem('價格(高到低)')\">價格(高到低)</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <ul class=\"row shop-body list-unstyled flex-wrap bg-white pt-5 pb-2 mb-0\">\n          <h3 class=\"text-center w-100\" v-if=\"searchList.length <=0\">尚無任何商品</h3>\n          <ShopItem v-for=\"(item, index) in searchList\" :key=\"index\" :item=\"item\" :index=\"index\"/>\n        </ul>\n    </div>\n</template>\n\n<script>\nimport ShopItem from '../shared/ShopItem'\n\nexport default {\n  data () {\n    return {\n      dataStick: [],\n      seletTxt: '價格(低到高)'\n    }\n  },\n  components: { ShopItem },\n  computed: {\n    searchList () {\n      if (this.$root.productSearchData) {\n        return this.$root.productSearchData\n      } else {\n        return this.dataStick\n      }\n    }\n  },\n  created () {\n    this.gameStick()\n  },\n  methods: {\n    gameStick: function () {\n      const filterStick = this.$root.productsData.filter(item => {\n        return item.Category.search('遊戲配件') !== -1\n      })\n      this.dataStick = filterStick.sort(function (a, b) {\n        return a.Price - b.Price\n      })\n    },\n    isRentMethod () {\n      const filterStatus = this.dataStick.filter(data => {\n        return data.Status.search('可出租') !== -1\n      })\n      this.$root.productSearchData = filterStatus\n    },\n    isNew () {\n      const filterNew = this.dataStick.sort(function (a, b) {\n        return Date.parse(b.PublishDate.replace(/-/g, '/')) - Date.parse(a.PublishDate.replace(/-/g, '/'))\n      })\n      this.$root.productSearchData = filterNew\n    },\n    isCheap () {\n      const filterPrice = this.dataStick.filter(data => {\n        return data.OriginPrice - data.Price > 0\n      })\n      this.$root.productSearchData = filterPrice\n    },\n    isAllItem (txt) {\n      this.seletTxt = txt\n      this.$root.productSearchData = this.dataStick.sort(function (a, b) {\n        return a.Price - b.Price\n      })\n    },\n    turnAllItem (txt) {\n      this.seletTxt = txt\n      this.$root.productSearchData = this.dataStick.sort(function (a, b) {\n        return b.Price - a.Price\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/front_side"}]}