A,IAAA,KAfA,mBAeA;AACA,UAAA,SAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,WAAA,CAAA,OAAA,CAAA,IAAA,EAAA,GAAA,CAAA,IAAA,IAAA,CAAA,KAAA,CAAA,CAAA,CAAA,WAAA,CAAA,OAAA,CAAA,IAAA,EAAA,GAAA,CAAA,CAAA;AACA,OAFA,CAAA;AAGA,WAAA,KAAA,CAAA,iBAAA,GAAA,SAAA;AACA,KApBA;AAqBA,IAAA,OArBA,qBAqBA;AACA,UAAA,WAAA,GAAA,KAAA,QAAA,CAAA,MAAA,CAAA,UAAA,IAAA,EAAA;AACA,eAAA,IAAA,CAAA,WAAA,GAAA,IAAA,CAAA,KAAA,GAAA,CAAA;AACA,OAFA,CAAA;AAGA,WAAA,KAAA,CAAA,iBAAA,GAAA,WAAA;AACA,KA1BA;AA2BA,IAAA,SA3BA,qBA2BA,GA3BA,EA2BA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA,KAhCA;AAiCA,IAAA,WAjCA,uBAiCA,GAjCA,EAiCA;AACA,WAAA,QAAA,GAAA,GAAA;AACA,WAAA,KAAA,CAAA,iBAAA,GAAA,KAAA,QAAA,CAAA,IAAA,CAAA,UAAA,CAAA,EAAA,CAAA,EAAA;AACA,eAAA,CAAA,CAAA,KAAA,GAAA,CAAA,CAAA,KAAA;AACA,OAFA,CAAA;AAGA;AAtCA;AApBA,CAAA","sourcesContent":["<template>\n    <div>\n        <ul class=\"row shop-body list-unstyled flex-wrap bg-white pt-5 pb-2 mb-0\">\n          <h3 class=\"text-center w-100\" v-if=\"searchList.length <=0\">尚無任何商品</h3>\n          <PerProduct  v-for=\"(item, index) in searchList\" :key=\"index\" :item=\"item\" :index=\"index\"/>\n        </ul>\n    </div>\n</template>\n\n<script>\n\nimport PerProduct from '../back_side/PerProduct'\n\nexport default {\n  data () {\n    return {\n      dataGame: [],\n      seletTxt: '價格(低到高)'\n    }\n  },\n  components: { PerProduct },\n  computed: {\n    searchList () {\n      if (this.$root.productSearchData) {\n        return this.$root.productSearchData\n      } else {\n        return this.dataGame\n      }\n    }\n  },\n  created () {\n    this.game()\n  },\n  methods: {\n    game: function () {\n      const filterGame = this.$root.productsData.filter(item => {\n        return item.Category.search('遊戲片') !== -1\n      })\n      this.dataGame = filterGame.sort(function (a, b) {\n        return a.Price - b.Price\n      })\n    },\n    isRentMethod () {\n      const filterStatus = this.dataGame.filter(data => {\n        return data.Status.search('可出租') !== -1\n      })\n      this.$root.productSearchData = filterStatus\n    },\n    isNew () {\n      const filterNew = this.dataGame.sort(function (a, b) {\n        return Date.parse(b.PublishDate.replace(/-/g, '/')) - Date.parse(a.PublishDate.replace(/-/g, '/'))\n      })\n      this.$root.productSearchData = filterNew\n    },\n    isCheap () {\n      const filterPrice = this.dataGame.filter(data => {\n        return data.OriginPrice - data.Price > 0\n      })\n      this.$root.productSearchData = filterPrice\n    },\n    isAllItem (txt) {\n      this.seletTxt = txt\n      this.$root.productSearchData = this.dataGame.sort(function (a, b) {\n        return a.Price - b.Price\n      })\n    },\n    turnAllItem (txt) {\n      this.seletTxt = txt\n      this.$root.productSearchData = this.dataGame.sort(function (a, b) {\n        return b.Price - a.Price\n      })\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/components/back_side"}]}